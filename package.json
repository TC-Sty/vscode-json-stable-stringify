{
  "name": "vscode-json5-stable-stringify",
  "version": "1.2.0",
  "displayName": "JSON5 Stable Sort",
  "description": "JSON5 object sort/document formatter using json-stable-stringify.",
  "license": "MIT",
  "publisher": "MatthiasStyrsky",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "sort",
    "json5",
    "format"
  ],
  "homepage": "https://github.com/tc-sty/vscode-json-stable-stringify",
  "qna": "https://github.com/tc-sty/vscode-json-stable-stringify/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tc-sty/vscode-json-stable-stringify"
  },
  "bugs": {
    "url": "https://github.com/tc-sty/vscode-json-stable-stringify/issues"
  },
  "activationEvents": [
    "onCommand:extension.vscode-json5-stable-stringify.jsonStableSort"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.vscode-json5-stable-stringify.jsonStableSort",
        "title": "Sort JSON5 (Stable)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "@types/sinon": "^7.5.2",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "sinon": "^9.0.1",
    "tar": "^6.0.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "vscode": "^1.1.36",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/json5": "0.0.30",
    "json-stable-stringify": "^1.0.1",
    "json5": "^2.1.3"
  }
}
